  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./express-session.d.ts"]
  },
  "include": ["src/server/**/*", "src", "**/*.tsx"],
